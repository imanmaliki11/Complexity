#include <bits/stdc++.h>
using namespace std;
int main () {
	long int n;
	string kata;
	cin >> n;
	while(n >0) {
		cin >> kata;
		if(kata.length()==5) cout << 3 << endl;
		else {
			if((kata[0]=='o' || (kata[1]=='n' && kata[2]=='e')) && (kata[1]=='n' || kata[2]=='e')) cout << 1 << endl;
			else cout << 2 << endl;
		}
		n--;
	}
}
